{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "^/favicon\\.ico$", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "redirects": [
    { "source": "http://www.brookwellindustries.in/(.*)", "destination": "https://brookwellindustries.in/$1", "permanent": true },
    { "source": "https://www.brookwellindustries.in/(.*)", "destination": "https://brookwellindustries.in/$1", "permanent": true }
  ]
}


